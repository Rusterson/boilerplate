{
  "presets": ["next/babel", "@babel/preset-typescript"],
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "ssr": true,
        "displayName": true
      }
    ]
  ],
  "env": {
    "test": {
      "pluging": [
        "bable-plugin-styled-components",
        {
          "ssr": false,
          "displayName": false
        }
      ]
    }
  }
}
